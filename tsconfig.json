{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    // "module": "commonjs",
    "module": "preserve",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".", //Added
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@mui/*": [
        "node_modules/@mui/*" //Added
      ]
    },
    "types": [ //Added
      "@types/node",
      "@types/react",
      "@mui/x-data-grid/themeAugmentation",
      "jest" // Added
    ],
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/**/*" //Added
  ],
  "exclude": [
    "node_modules"
  ]
}